{
  "name": "nuxtdoc",
  "version": "1.0.0",
  "description": "A setup to build beautiful documentation with Nuxt and Storyblok",
  "author": "Dominik Angerer <dominikangerer1@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
