    "storyblok-nuxt": "1.2.0"
  },
  "devDependencies": {
    "cheerio": "^1.0.0-rc.3",
    "netrc": "^0.1.4",
    "node-sass": "^4.14.1",
    "sass-loader": "^8.0.2"
  }
